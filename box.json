{
  "chmod": "0700",
  "main": "PHPTestsCalls",
  "directories": [
    "config",
    "src"
  ],
  "finder": [
    {
      "notName": "/LICENSE|.*\\.md|.*\\.dist|Makefile|composer\\.json|composer\\.lock/|services.yaml",
      "exclude": [
        "doc",
        "test",
        "test_old",
        "tests",
        "Tests",
        "vendor-bin"
